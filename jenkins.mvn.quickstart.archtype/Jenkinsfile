pipeline {

    agent any
    stages {
        stage('Maven install') {
        steps {
          withMaven(globalMavenSettingsConfig: '--- Use system default settings or file path ---', jdk: 'JDK installtion', maven: 'Maven', mavenSettingsConfig: 'de015ca0-a0d2-485c-9560-340bc00a314c') {
            sh 'mvn clean install'
          }
        }
      }
    }


}
